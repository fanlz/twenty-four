{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
    },
    "rules": {
        "no-console": "off",
        "strict": ["error", "global"],
        "curly": "warn"
    }
}
